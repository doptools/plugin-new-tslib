{
  "name": "@doptools/cli",
  "description": "build and deployment tools",
  "version": "0.0.0-dev.7",
  "author": "Dan Erdelt @myrddraall",
  "bin": {
    "dops": "./bin/run"
  },
  "schematics": "./src/collection.json",
  "bugs": "https://github.com/doptools/cli/issues",
  "dependencies": {
    "@angular-devkit/schematics": "^11.2.1",
    "@doptools/plugin-version": "^0.0.0-dev.5",
    "@doptools/tslib-cli-core": "^0.0.0-dev.4",
    "@doptools/version": "^0.0.1-alpha.0",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-plugins": "^1.9.5",
    "@oclif/plugin-update": "^1.3.10",
    "@octokit/rest": "^18.2.0",
    "cli-ux": "^5.5.1",
    "jsonc": "^2.0.0",
    "minimist": "^1.2.5",
    "npm-package-arg": "^8.1.1",
    "resolve-package-path": "^3.1.0",
    "tslib": "^2.1.0",
    "type-fest": "^0.21.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/node": "14",
    "@types/npm-package-arg": "^6.1.0",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/doptools/cli",
  "keywords": [
    "oclif"
  ],
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "dops",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-update",
      {
        "type": "user",
        "name": "@doptools/plugin-version",
        "url": "@doptools/plugin-version",
        "tag": "latest"
      }
    ],
    "hooks": {
      "version:apply": "./lib/hooks/version/apply",
      "init": "./lib/hooks/init/load-local-plugins"
    }
  },
  "repository": "doptools/cli",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
